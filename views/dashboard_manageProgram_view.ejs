<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/html">
<head>
    <title>Manage Programs</title>
    <link rel='stylesheet' href='/stylesheets/style.css'/>
    <link rel="stylesheet" href="/stylesheets/semantic.min.css"/>
    <script src="/javascripts/jquery-3.1.0.min.js"></script>
    <script src="/javascripts/semantic.js"></script>
    <script src="/javascripts/script.managePrograms.js"></script>
</head>
<body>
<h1 class="ui teal header" style="margin-top: 32px ; margin-left: 32px ; margin-bottom: 32px">Dashboard</h1>
<div class="ui divider"></div>
<% semesters.forEach(function(semester,index){ %>
<div class="ui horizontal divider">Semester <%= index+1 %></div>
<div class="ui stacked segment">
    <table class="ui celled table">
        <tr>
            <th>
                Course
            </th>
        </tr>
        <% semester.forEach(function(course){ %>
            <tr>
                <td><%= course %></td>
            </tr>
        <% }); %>
    </table>
</div>
<% }); %>

</body>
</html>